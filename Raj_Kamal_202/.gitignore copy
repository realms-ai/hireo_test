# Ignore environment variable files
.env
.env.local
.env.development
.env.production
.env.test

# Ignore node_modules directory
node_modules/

# Ignore log files
*.log
logs/

# Ignore build output
dist/
build/

# Ignore IDE/editor-specific files
.vscode/
.idea/
*.swp

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore npm/yarn debug files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore coverage output
coverage/

# Ignore package-lock or yarn.lock (optional, only if not used in your workflow)
# package-lock.json
# yarn.lock

